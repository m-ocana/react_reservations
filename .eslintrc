{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": [2, "never"],
    "import/no-extraneous-dependencies": 0,
    "react/prefer-stateless-function": 0,
    "semi": [2, "never"]
  }
}
